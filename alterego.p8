pico-8 cartridge // http://www.pico-8.com
version 36
__lua__
-- alter ego
-- by simon arnold

-- flags
-- 0=ground
-- 1=wall
-- 2=ladder
-- 3=anim1
-- 4=anim2
-- 5=player (not really used)

-- utilities
#include utilities/collide_map.lua
#include utilities/is_touching.lua
#include utilities/has_flag.lua
#include utilities/sleep.lua

--scenes
#include scenes/credits.lua
#include scenes/menu.lua
#include scenes/play.lua
#include scenes/gameover.lua
#include scenes/win.lua

--components
#include vars.lua
#include game.lua
#include map.lua
#include level.lua
#include player.lua
#include ego.lua
#include bridges.lua
#include purple_targets.lua
#include green_targets.lua
#include skulls.lua
#include flying_skulls.lua

function _init()
  set_vars()
  init_game()
end

function _update()
  update_map()
 
  if game.scr.active=="menu" then
    update_menu()
  elseif game.scr.active=="play" then
    update_play()
  end
end

function _draw()
  cls()
  map(game.cam.x,game.cam.y)
 
  if game.scr.active=="credits" then
    draw_credits()
  elseif game.scr.active=="menu" then
    draw_menu()
  elseif game.scr.active=="over" then
    draw_gameover()
  elseif game.scr.active=="win" then
    draw_win()
  elseif game.scr.active=="play" then
    draw_play()
	end
end

